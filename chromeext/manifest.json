{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "Save For Later",
  "description": "Save your favorite articles to read later!",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "oauth2": {
    "client_id": "1045180051797-lkd2vp5kqeptim8f25fv1ag7r9etvej0.apps.googleusercontent.com",
    "scopes": [""]
  },
  "key": "",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
  },
  "permissions": ["activeTab", "identity"]
}
